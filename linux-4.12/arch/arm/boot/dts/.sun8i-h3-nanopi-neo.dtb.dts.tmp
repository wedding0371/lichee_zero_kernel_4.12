  pinctrl-0 = <&leds_npi>, <&leds_r_npi>;

  status {
   label = "nanopi:blue:status";
   gpios = <&pio 0 10 0>;
   linux,default-trigger = "heartbeat";
  };

  pwr {
   label = "nanopi:green:pwr";
   gpios = <&r_pio 0 10 0>;
   default-state = "on";
  };
 };

 r_gpio_keys {
  compatible = "gpio-keys";
  input-name = "k1";
  pinctrl-names = "default";
  pinctrl-0 = <&sw_r_npi>;

  k1@0 {
   label = "k1";
   linux,code = <116>;
   gpios = <&r_pio 0 3 1>;
  };
